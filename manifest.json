{
   "background": {
      "page": "tablisteditor.html",
      "persistent": false
   },
   "chrome_url_overrides": {
      "newtab": "tablisteditor.html"
   },

   "content_security_policy": "script-src 'self'; object-src 'self'",
   "description": "SKKU GOOGLE EXTENSION DEVELOPMENT PROJECT",
   "manifest_version": 2,
   "name": "tabAnalyzer",
   "options_page": "options.html",
   "permissions": ["tabs", "activeTab", "storage", "unlimitedStorage", "contextMenus"],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.0",

   "browser_action": {
      "default_popup": "popup.html",
      "default_title": "Tab Analyzer",
      "default_icon": {
         "16": "icon.png",
         "32": "icon.png",
         "48": "icon.png",
         "128": "icon.png"
      }
   },
   "icons": {
      "16": "icon.png",
      "32": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
   }
}